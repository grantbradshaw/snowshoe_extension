{
  "manifest_version": 2,
  "name": "gscrape",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-2.2.0.min.js", "base_module.js", "helpers.js", "main.js"],
      "css": ["css/content.css"],
      "all-frames": true
    }
  ], 
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }, 
  "permissions": [
    "http://localhost/*"
  ],
  "web_accessible_resources": [
    "frame.html"
  ]
}